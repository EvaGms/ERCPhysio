<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Symfony\Bundle\MakerBundle\Entity\Hypotheses" table="hypotheses">
    <indexes>
      <index name="FK_hypotheses_id_cas_clinique" columns="id_cas_clinique"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="hypothese" type="text" column="hypothese" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idCasClinique" target-entity="CasClinique" fetch="LAZY">
      <join-columns>
        <join-column name="id_cas_clinique" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
